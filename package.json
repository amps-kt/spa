{
  "name": "allocation-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tc": "tsc",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "eval $(grep '^DATABASE_URL' .env) && prisma db push",
    "db:drop": "eval $(grep '^DATABASE_URL' .env) && prisma migrate reset",
    "db:populate": "pnpm run db:push --force-reset && eval $(grep '^DATABASE_URL' .env) && tsx src/lib/scripts/seed.ts",
    "docker:dev": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.override.yml up -d",
    "docker:prod": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.prod.override.yml up -d",
    "dev:patch": "tsx src/lib/scripts/patch.ts",
    "schema:change": "pnpm run db:generate && pnpm run db:push"
  },
  "dependencies": {
