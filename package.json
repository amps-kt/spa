{
  "name": "allocation-app",
  "version": "0.2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:email": "dotenvx run -- email dev --dir ./src/emails/messages -p 3001",
    "build:email": "dotenvx run -- email build --dir ./src/emails/messages",
    "start:email": "dotenvx run -- email start",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "tc": "tsc",
    "postinstall": "prisma generate --no-hints",
    "db:generate": "prisma generate",
    "db:generate-push": "pnpm run db:generate && pnpm run db:push",
    "db:studio": "prisma studio",
    "db:push": "eval $(grep '^DATABASE_URL' .env) && prisma db push",
    "db:drop": "eval $(grep '^DATABASE_URL' .env) && prisma migrate reset",
    "db:seed": "eval $(grep '^DATABASE_URL' .env) && tsx src/db/seed.ts",
    "docker:db": "docker compose -f docker/docker-compose.yml up -d",
    "docker:prod": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.prod.override.yml up -d",
    "dev:patch": "tsx src/db/scripts/patch.ts",
    "clearLogs": "rm *.log"
  },
